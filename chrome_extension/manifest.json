{
  "name": "Autho",
  "description": "Autho",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "action": {
    "default_icon": "icons/icon-256.png"
  },
  "icons": {
    "16": "icons/icon-32.png",
    "48": "icons/icon-64.png",
    "128": "icons/icon-128.png",
    "256": "icons/icon-256.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "css": ["content.css"]
    }
  ],
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  }
}
